let g:rsenseHome = "/home/ecounts/opt/rsense-0.3"
let g:rsenseUseOmniFunc = 1

:colorscheme desertEx

set nocompatible

set ruler
set smartindent
set autoindent
set smarttab autoindent
set expandtab
set tabstop=2
set shiftwidth=2
set cindent
set cinkeys=0{,0},:,0#,!^F

:set incsearch
:syntax enable

set backspace=indent,eol,start

map <F2> :NERDTreeToggle<CR>
